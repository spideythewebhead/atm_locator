  "version": "0.2.0",
  "configurations": [
    {
      "name": "app (emulator)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=baseUrl=http://10.0.2.2:55440/"]
    },
    {
      "name": "app (web)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=baseUrl=http://localhost:55440/"]
    },
    {
      "name": "app (profile mode)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    }
  ]
